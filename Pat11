public class Pat11 {
public static void main(String[] args) {
	Pat11 ss = new Pat11();
	ss.calmethod(5);
	}
	int k2=0;
private void calmethod(int i) {
	if(1<=i)
	{
	col(1,i);
	k2++;
	col2(k2,1);
	System.out.println();
	calmethod(i-1);
	}
	}
private void col(int k2, int i) {
		if(k2<i)
		{
			System.out.print(k2);
			col(k2,i-1);
		}
		}

private void col2(int k2, int m2) {
		if(m2<=k2&&k2<6)
		{
			System.out.print(k2);
			m2++;
			col2(k2,m2);
		}
	}

}
