public class Rec3 {

	public static void main(String[] args) {
		Rec3 cc = new Rec3();
		cc.calmethod(5);

	}
int n=11;
	private void calmethod(int i) {
	if(i>=1)
	{
		n=n-2;
		col(i,n);
		System.out.println();
		calmethod(i-1);
	}
	}
	private void col(int j, int n) {
	if(j<=n)
	{
		System.out.print(j);
		col(j+1,n);
	}
		
	}

}
