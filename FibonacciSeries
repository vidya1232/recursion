public class FibonacciSeries {
	 int f1=0;
	 int s1=1;
	 int t1;
	public static void main(String[] args) {
	  // TODO Auto-generated method stub
	  FibonacciSeries fb =new FibonacciSeries();
	  fb.fibonacci(10);
	}
	public void fibonacci(int count)
	{
	if(count>=1)
	{
	t1=f1+s1;
	System.out.println(f1+"+"+s1+"="+t1);
	f1=s1;
	s1=t1;
	
	
	fibonacci(count-1);
	}
	
	}
	}
