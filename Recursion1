public class Recursion1 {

	public static void main(String[] args) {
	Recursion1 ss = new Recursion1();
	ss.method(10);

	}
	int v=5;int k=11;
	public  void method(int i)
	{
		if(i>=1)
		{
		v--;
		k=k-v;
		 col(i,k);
		 System.out.println();
		i=i-v;
		method(i);
		}
	}
	private void col(int j, int n) {
	if(j>=n)
	{
		System.out.print(j);
		col(j-1,n);
	}
	}

}
