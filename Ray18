public class Ray18
{
public static void main(String[]args)
{
int a[]={10,20,40,30,50,60,70};//70,60,50,30,40,20,10//
for(int i=0,j=a.length-1;i<j;i++,j--)//i=1,j=5//i=2,,j=4,i=3,j=3
{
int temp=a[i];//10,20,40
a[i]=a[j];//=70,60,50
a[j]=temp;//a[6]=10,20,40
}
for(int i=0;i<a.length;i++)
{
	System.out.print(a[i]+" ");
}
}
}
